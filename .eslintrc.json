{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb/base",
    "prettier",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-alert": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "no-tabs": "off",
    "func-names": "off",
    "consistent-return": "off"
  }
}
